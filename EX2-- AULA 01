#include <iostream>
using namespace std;

int main(int argc, char** argv) 
{
	int total=6;
	float valores[6];
	
	cout<< ("digite 6 valores:");
	
	for(int i=0;i<total;i++)
	{
	
		cin>> valores[i];
	}
	
	for (int i=0; i<total; i++)
	{
		cout<< ("nÃºmeros digitados:" , valores[i]);
	}
	
	

	return 0;
}
