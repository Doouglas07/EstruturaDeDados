#include <iostream>
#include<string>
using namespace std;

int main()
{
    int total= 10;
    float valor[total]; 

    for (int i = 0; i < total; i++){ 
        cin >> valor[i];
        if (valor[i] < 0)
            valor[i] = 0;
    }

    for (int i = 0; i < total; i++) { 

        cout << valor[i] << endl;
    }

    return 0;
}
